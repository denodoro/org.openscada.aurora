<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.openscada.ds.storage.jdbc.internal.EntryBlob" table="datastore">

		<composite-id>
			<key-property name="nodeId"></key-property>
			<key-property name="instance"></key-property>
			<generator class="assigned"/>
		</composite-id>
		
		<property name="data" type="binary" />

	</class>
	
</hibernate-mapping>